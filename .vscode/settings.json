{
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.codeActionsOnSaveTimeout": 20000,
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "eslint.format.enable": true,
    "eslint.probe": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue"
    ],
    "eslint.run": "onSave",
    "eslint.codeActionsOnSave.mode": "problems",
}